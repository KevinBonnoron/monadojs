{
  "name": "monadojs",
  "version": "0.0.26",
  "description": "A library to perform operations on arrays and objects at once",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./types/monadojs.d.ts",
        "default": "./dist/monadojs.mjs"
      },
      "require": {
        "types": "./types/monadojs.d.cts",
        "default": "./dist/monadojs.cjs"
      }
    }
  },
  "types": "./dist/monadojs.d.ts",
  "main": "./dist/monadojs.cjs",
  "packageManager": "yarn@1.22.15",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "vitest --watch=false",
    "test:watch": "vitest",
    "bench": "vitest bench --watch=false",
    "bench:watch": "vitest bench",
    "typecheck": "vitest typecheck --watch=false",
    "typecheck:watch": "vitest typecheck",
    "prebuild": "rimraf --glob dist/**",
    "build": "rollup -c",
    "lint": "eslint . --fix",
    "prepack": "yarn build",
    "release": "release-it --only-version"
  },
  "keywords": [],
  "author": "Kevin Bonnoron <2421321+KevinBonnoron@users.noreply.github.com>",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-terser": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.18.17",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "release-it": "^15.5.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.18.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "repository": "https://github.com/KevinBonnoron/monadojs.git",
  "private": false
}
